    "from eliza import Eliza\n",
    "\n",
    "eliza_bot = Eliza()\n",
    "eliza_bot.load('doctor.txt')\n",
    "\n",
    "print(eliza_bot.initial())\n",
    "\n",
    "while True:\n",
    "    said = input('> ')\n",
    "    response = eliza_bot.respond(said)\n",
    "    if response is None:\n",
    "        break\n",
    "    print(response)\n",
    "print(eliza_bot.final())"
